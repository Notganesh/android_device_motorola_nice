# Extcon
genfscon sysfs /devices/platform/soc/soc:extcon_usb/extcon/extcon0/cable.0/name  u:object_r:sysfs_extcon:s0
genfscon sysfs /devices/platform/soc/soc:extcon_usb/extcon/extcon0/cable.1/name  u:object_r:sysfs_extcon:s0

# Fingerprint
genfscon sysfs /devices/virtual/fingerprint/fpc_spi/clk_enable        u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/virtual/fingerprint/fpc_spi/compatible_all    u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/virtual/fingerprint/fpc_spi/device_prepare    u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/virtual/fingerprint/fpc_spi/hw_reset          u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/virtual/fingerprint/fpc_spi/irq               u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/virtual/fingerprint/fpc_spi/wakeup_enable     u:object_r:sysfs_fingerprint:s0

# Input Devices
genfscon sysfs /devices/virtual/input u:object_r:vendor_sysfs_input:s0

# Memory
genfscon proc /sys/vm/lowmem_reserve_ratio    u:object_r:proc_watermark_boost_factor:s0
genfscon proc /sys/vm/min_free_kbytes         u:object_r:proc_watermark_boost_factor:s0

# Motorola Specific filesystem entries
genfscon proc /bootinfo u:object_r:vendor_proc_hw:s0
genfscon proc /config u:object_r:vendor_proc_hw:s0
genfscon proc /hw u:object_r:vendor_proc_hw:s0

